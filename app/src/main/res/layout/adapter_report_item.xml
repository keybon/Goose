<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginRight="@dimen/activity_margin_30"
    android:layout_marginLeft="@dimen/activity_margin_30"
    >


    <TextView
        android:id="@+id/report_item_name"
        android:layout_width="match_parent"
        android:layout_height="@dimen/activity_margin_40"
        android:textSize="@dimen/activity_textsize_16"
        android:textColor="@color/common_black"
        android:gravity="center_vertical"
        android:text="广告"
        />

    <CheckBox
        android:id="@+id/report_item_checkbox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:focusable="false"

        />

</RelativeLayout>